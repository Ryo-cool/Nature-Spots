{
  "name": "nature-spots",
  "version": "1.0.0",
  "description": "Nature-Fix",
  "author": "cool",
  "private": true,
  "resolutions": {
    "strip-ansi": "6.0.1"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "generate": "nuxi generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "test:unit": "vitest",
    "format": "prettier --write \"**/*.{js,ts,vue,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,md}\"",
    "check": "yarn lint && yarn type-check",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.10.1",
    "@vueuse/core": "^13.0.0",
    "crypto-js": "^4.0.0",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.1",
    "vue-i18n": "^11.1.2",
    "vue3-google-map": "^0.21.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^9.3.1",
    "@types/crypto-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/eslint-config-typescript": "^14.4.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "lint-staged": "14.0.1",
    "nuxt": "^3.10.3",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "typescript": "^5.7.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.2.0",
    "vuetify": "^3.7.16"
  },
  "lint-staged": {
    "front/**/*.{js,ts,vue}": [
      "yarn --cwd front lint --fix --cache --cache-location ../.cache/eslint"
    ],
    "front/**/*.{json,md}": [
      "yarn --cwd front prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
